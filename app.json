{
  "expo": {
    "name": "PeerConnect",
    "slug": "peerconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0",

      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC74A0kHLwgniNk9iJtne5q3TMOvgmG0Wg"
        }
      },

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-image-picker",
      "expo-camera",
      "expo-contacts",
      "expo-location",
      "expo-notifications",
      "expo-sms"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e62d5b22-8a56-4926-8168-c294a693641e"
      }
    },
    "owner": "dolendeori"
  }
}
